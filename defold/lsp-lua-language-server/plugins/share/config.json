{
    "$schema": "https://raw.githubusercontent.com/sumneko/vscode-lua/master/setting/schema.json",
    "diagnostics": {
        "workspaceEvent": "None",
        "enable": true
    },
    "Lua": {
        "runtime": {
            "version": "Lua 5.1"
        },
        "workspace": {
            "library": [
                "${3rd}/Defold/library",
                ".internal"
            ]
        }
    },
    "window": {
        "progressBar": false,
        "statusBar": false
    }
}
